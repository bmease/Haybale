<?xml version="1.0"?>
<view width="110" height="205" title="Meta - Fellow">
    <control type="layout">

        <control type="button" name="Add" left="5" top="5" width="100" height="20" text="Add" setstate="ChatPasteCommand" actionexpr="setvar[command, \#fellow add ]" />
        <control type="button" name="Close" left="5" top="30" width="100" height="20" text="Close" setstate="ChatCommand" actionexpr="setvar[command, \#fellow close]" />
        <control type="button" name="Create" left="5" top="55" width="100" height="20" text="Create" setstate="ChatCommand" actionexpr="setvar[command, \#fellow create];setvar[Force, say]" />
        <control type="button" name="Disband" left="5" top="80" width="100" height="20" text="Disband" setstate="ChatCommand" actionexpr="setvar[command, \#fellow disband]" />
        <control type="button" name="Leader" left="5" top="105" width="100" height="20" text="Leader" setstate="ChatCommand" actionexpr="setvar[command, \#fellow leader]" />
        <control type="button" name="Nearby" left="5" top="130" width="100" height="20" text="Nearby" setstate="ChatCommand" actionexpr="setvar[command, \#fellow nearby]" />
        <control type="button" name="Open" left="5" top="155" width="100" height="20" text="Open" setstate="ChatCommand" actionexpr="setvar[command, \#fellow open]" />
        <control type="button" name="Quit" left="5" top="180" width="100" height="20" text="Quit" setstate="ChatCommand" actionexpr="setvar[command, \#fellow quit]" />

    </control>
</view>